package chemicalReactions.chemicalEquilibriaum;

import java.util.*;
import thermo.component.*;
/**
 *
 * @author  Even Solbraa
 * @version 
 */
public class ReferencePotComparator implements Comparator{

    public int compare(Object o1, Object o2){
        
        System.out.println("sorting.....");
        
        System.out.println("sorting.....");
     double v1 = ((ComponentInterface) o1).getReferencePotential();  
     double v2 = ((ComponentInterface) o2).getReferencePotential(); 
     
     int ans = v1>=v2 ? 1:0;
     
     return ans;
    }

}
