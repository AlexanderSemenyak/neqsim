package physicalProperties.mixingRule;

import thermo.*;
import thermo.phase.*;
/**
 *
 * @author  esol
 * @version
 */
public interface PhysicalPropertyMixingRuleInterface {
    public double getViscosityGij(int i, int j);
    public void setViscosityGij(double val, int i, int j);
    public double getConductivityAij(int i, int j);
    public void setConductivityAij(double val, int i, int j);
    public void initMixingRules(PhaseInterface phase);
}

