package thermo.mixingRule;

import thermo.component.*;
import thermo.phase.*;
/**
 *
 * @author  Even Solbraa
 * @version
 */
public interface ElectrolyteMixingRulesInterface {
    
    double calcW(PhaseInterface phase, double temperature, double pressure, int numbcomp);
    double calcWT(PhaseInterface phase, double temperature, double pressure, int numbcomp);
    double calcWTT(PhaseInterface phase, double temperature, double pressure, int numbcomp);
    double calcWiT(int compNumb, PhaseInterface phase, double temperature, double pressure, int numbcomp);
    double calcWi(int compNumb, PhaseInterface phase, double temperature, double pressure, int numbcomp);
    double calcWij(int compNumbi, int compNumj, PhaseInterface phase, double temperature, double pressure, int numbcomp);
    public void setWijParameter(int i, int j, double value);
    public double getWijParameter(int i, int j);
    public void setWijT1Parameter(int i, int j, double value);
    public double gettWijT1Parameter(int i, int j);
    public void setWijT2Parameter(int i, int j, double value);
    public double gettWijT2Parameter(int i, int j);
}

