import thermo.system.*;
import thermodynamicOperations.*;

/**
 *
 * @author  esol
 * @version
 */
public class ModelTest {
    
    /** Creates new TPflash */
    public ModelTest() {
    }
    public static void main(String args[]){
        //SystemInterface testSystem = new SystemFurstElectrolyteEos(280.15,10.00);
        //SystemInterface testSystem = new SystemSrkCPA(320.15,10.00);
        
        SystemInterface testSystem = new SystemSrkEos(280.15,10.00);
        // SystemInterface testSystem = new SystemPrEos(280.15,10.00);
        
        ThermodynamicOperations testOps = new ThermodynamicOperations(testSystem);
        //testSystem.addComponent("methane", 10);
        testSystem.addComponent("methane", 10.0);
        testSystem.addComponent("water", 50);
        // testSystem.addComponent("MDEA", 10);
        // 1- orginal no interaction 2- classic w interaction
        // 3- Huron-Vidal 4- Wong-Sandler
        //testSystem.chemicalReactionInit();
        testSystem.createDatabase(true);
        testSystem.setMixingRule(5);
        try{
           testOps.TPflash();
        }
        catch(Exception e){
            
        }
        // testOps.displayResult();
        
        thermo.ThermodynamicModelTest testModel = new thermo.ThermodynamicModelTest(testSystem);
        testModel.runTest();
    }
}
