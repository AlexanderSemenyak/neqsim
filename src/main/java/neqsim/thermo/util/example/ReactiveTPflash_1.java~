package thermo.util.example;
/*
 * TPflash.java
 *
 * Created on 27. september 2001, 09:43
 */

import thermo.system.*;

/**
 *
 * @author  esol
 * @version
 */
public class ReactiveTPflash {
    
    /** Creates new TPflash */
    public ReactiveTPflash() {
    }
    public static void main(String args[]){
        SystemInterface testSystem = new SystemFurstElectrolyteEos(298.15,1.01325);
        //SystemInterface testSystem = new SystemSrkEos(298.15,1.01325);
        
        testSystem.addComponent("water", 1.0);
        //testSystem.addComponent("methane", 1.0);
        testSystem.addComponent("Cl-", 0.018018);
        testSystem.addComponent("Na+", 0.018018);
        //testSystem.addComponent("MDEA", 0.1);
        testSystem.setMixingRule(1);
        testSystem.init(0);
        testSystem.init(3);
        testSystem.init(3);
        
        thermo.ThermodynamicModelTest testModel = new thermo.ThermodynamicModelTest(testSystem);
        testModel.runTest();
    }
}
