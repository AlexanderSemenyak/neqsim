package thermo.util.example;
/*
 * TPflash.java
 *
 * Created on 27. september 2001, 09:43
 */

import thermo.system.*;
import thermodynamicOperations.*;

/**
 *
 * @author  esol
 * @version
 */
public class FreezingPoint {
    
    /** Creates new TPflash */
    public FreezingPoint() {
    }
    public static void main(String args[]){
        SystemInterface testSystem = new SystemSrkSchwartzentruberEos(270.15,1.00);
        //SystemInterface testSystem = new SystemSrkCPA(272.15,20.00);
        ThermodynamicOperations testOps = new ThermodynamicOperations(testSystem);
        testSystem.addComponent("methane",10.0);
        //testSystem.addComponent("n-heptane",10.0);
        // testSystem.addComponent("MEG", 1.000100210012);//,"kg/min");
        testSystem.addComponent("water", 10.01057);//, "kg/min");
       
        testSystem.createDatabase(true);
        testSystem.setSolidPhaseCheck(true);
        
        //testSystem.setMultiPhaseCheck(true);
        testSystem.setMixingRule(2);
        
        testSystem.init( 0);
        try{
            //testOps.freezingPointTemperatureFlash();
            // testOps.waterDewPointTemperatureFlash();
            //    testOps.dewPointTemperatureFlash();
              testOps.TPflash();
            //  testOps.dewPointTemperatureFlash();
            testSystem.display();
        }
        catch(Exception e){
            System.out.println(e.toString());
        }
    }
}
