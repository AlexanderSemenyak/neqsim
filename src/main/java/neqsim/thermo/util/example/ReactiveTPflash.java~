import thermo.system.*;
import thermodynamicOperations.*;
/**
 *
 * @author  esol
 * @version
 */
public class ReactiveTPflash {
    
    /** Creates new TPflash */
    public ReactiveTPflash() {
    }
    public static void main(String args[]){
        SystemInterface testSystem = new SystemFurstElectrolyteEos(313.15,10.1325);
        //SystemInterface testSystem = new SystemElectrolyteCPA(313.15,10.01325);
        // SystemInterface testSystem = new SystemSrkCPA(298.15,1.1325);
        //SystemInterface testSystem = new SystemSrkEos(298.15,1.01325);
        
        //  testSystem.addComponent("methane", 0.1);
        testSystem.addComponent("CO2", 0.07);
        testSystem.addComponent("water", 1.0);
        //testSystem.addComponent("Piperazine", 0.02);
          testSystem.addComponent("MDEA", 0.10);
        //        testSystem.addComponent("HCO3-", 0.1);
        //testSystem.addComponent("methanol", 0.05);
        testSystem.chemicalReactionInit();
        testSystem.createDatabase(true);
        testSystem.setMixingRule(4);
        testSystem.init(0);
        testSystem.init(1);
        ThermodynamicOperations ops = new ThermodynamicOperations(testSystem);
        try{
            ops.bubblePointPressureFlash(false);
            //ops.TPflash();
        }
        catch(Exception e){}
        testSystem.display();
        System.out.println("pH: " + testSystem.getPhase(1).getpH());
        System.out.println("pco2: " + testSystem.getPhase(0).getPressure()*testSystem.getPhase(0).getComponent("CO2").getx());
        //        System.out.println("act " + testSystem.getPhase(1).getActivityCoefficient(2,0));
        //        System.out.println("act " + testSystem.getPhase(1).getActivityCoefficient(2));
        //        System.out.println("act " + testSystem.getPhase(1).getActivityCoefficient(0));
        //       thermo.ThermodynamicModelTest testModel = new thermo.ThermodynamicModelTest(testSystem);
        //        testModel.runTest();
    }
}
