package thermo.util.example;
/*
 * TPflash.java
 *
 * Created on 27. september 2001, 09:43
 */

import thermo.system.*;
import thermodynamicOperations.*;
/**
 *
 * @author  esol
 * @version
 */
public class AmineFlash {
    
    /** Creates new TPflash */
    public AmineFlash() {
    }
    
    public static void main(String args[]){
        SystemInterface testSystem = new SystemFurstElectrolyteEos(273.15+25, 1.01325);
        testSystem.addComponent("CO2", 0.0000061);
        testSystem.addComponent("water", 1.0);
        //testSystem.addComponent("MDEA", 0.1);
        testSystem.chemicalReactionInit();
        testSystem.createDatabase(true);
        testSystem.setMixingRule(4);
        ThermodynamicOperations testOps = new ThermodynamicOperations(testSystem);
        try{
            testOps.bubblePointPressureFlash(false);
        }
        catch(Exception e){
            System.out.println("err " + e.toString());
        }
        System.out.println("pH "+ testSystem.getPhase(1).getpH());
        testSystem.display();
    }
}
