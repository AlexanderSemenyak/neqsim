import thermo.system.*;
import thermodynamicOperations.*;

/*
 *
 * @author  esol
 * @version
 */
public class TestPSRK {
    
    /** Creates new TPflash */
    public TestPSRK() {
    }
    public static void main(String args[]){
        //
        
        SystemInterface testSystem = new SystemPsrkEos(310,1.301325);
        //SystemInterface testSystem = new SystemSrkEos(273.15 + 55.0,1.301325);
        ThermodynamicOperations testOps = new ThermodynamicOperations(testSystem);
        
     //   testSystem.addComponent("methane", 0.01);
     //   testSystem.addComponent("methane", 0.001);
        testSystem.addComponent("water", 10.0);
        testSystem.addComponent("methanol", 10.0);
       // testSystem.addComponent("acetone", 10.0);
        testSystem.createDatabase(true);
        testSystem.setMixingRule("HV","UNIFAC");
     ///  testSystem.setMixingRule("HV","NRTL");
        testSystem.init(0);
        testSystem.init(1);
        
        try{
          //  testOps.TPflash();
            testOps.bubblePointPressureFlash(false);//(false);
        }
        catch(Exception e){
            System.out.println(e.toString());
        }
        testSystem.display();
    }
    
}
