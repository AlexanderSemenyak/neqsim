package thermo.util.parameterFitting.binaryInteractionParameterFitting.HuronVidalParameterFitting;

import thermo.system.*;
import thermodynamicOperations.*;
import statistics.parameterFitting.nonLinearParameterFitting.*;
import thermo.system.*;
import thermo.mixingRule.*;
import thermo.phase.*;

/**
 *
 * @author  Even Solbraa
 * @version
 */
public class BinaryHVParameterFittingToSolubilityData extends  HuronVidalFunction{
    
    int phase = 1;
    int type = 1;
    /** Creates new Test */
    public BinaryHVParameterFittingToSolubilityData() {
    }
    
    public BinaryHVParameterFittingToSolubilityData(int phase, int type) {
        this.phase = phase;
        this.type = type;
    }
    
    public double calcValue(double[] dependentValues){
        thermoOps.TPflash();
        if(type==1){
            //System.out.println("x " + system.getPhases()[1].getComponents()[0].getx());
            return system.getPhases()[phase].getComponents()[0].getx();
        }
        else{
            return system.getPhases()[phase].getComponents()[1].getx();
        }
    }
    
    public double calcTrueValue(double val){
        return val;
    }
}