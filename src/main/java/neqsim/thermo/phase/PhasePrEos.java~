package thermo.phase;

import thermo.component.*;

/** 
 *
 * @author  Even Solbraa
 * @version 
 */
public class PhasePrEos extends PhaseEos implements PhaseEosInterface{

  /** Creates new PhaseSrkEos */
  public PhasePrEos() {
     super();
     uEOS = 2;
     wEOS = -1;    
     delta1 = 1.0 + Math.sqrt(2.0);
     delta2 = 1.0 - Math.sqrt(2.0);
  }
  
  public Object clone(){
        PhasePrEos clonedPhase = null;
        try{
        clonedPhase = (PhasePrEos) super.clone();
        }
        catch(Exception e) 
        {
            e.printStackTrace(System.err);
        }
        
        return clonedPhase;
    }
  
  public void addcomponent(String componentName, double moles,double molesInPhase,  int compNumber){
  	super.addcomponent(molesInPhase);
        componentArray[compNumber] = new ComponentPR(componentName, moles, molesInPhase, compNumber);
  }
  
			
  

}