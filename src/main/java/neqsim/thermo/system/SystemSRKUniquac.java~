package thermo.system;

import thermo.phase.*;

/**
 *
 * @author  Even Solbraa
 * @version 
 */
public class SystemSRKUniquac extends SystemEos {

    /** Creates new SystemSRKUniquac */
    public SystemSRKUniquac() {
    }
    
    public SystemSRKUniquac(double T,double P) {
        super(T,P);
     
    phaseArray[0] = new PhaseGEUniquac();    
    phaseArray[1] = new PhaseSrkEos();
    phaseArray[2] = new PhaseSrkEos();
    }
  }
