package statistics.parameterFitting.util.example;

import thermo.system.*;
import thermodynamicOperations.*;
import statistics.parameterFitting.nonLinearParameterFitting.*;

/**
 *
 * @author  Even Solbraa
 * @version
 */
public class TestFunction extends LevenbergMarquardtFunction {
    
    /** Creates new Test */
    public TestFunction() {
    }
   
    public double calcValue(double[] dependentValues){
        return 3.0*params[0]*params[1]-2.0*params[0]*dependentValues[0]-dependentValues[0];
    }
    
    public void setFittingParams(int i, double value){
        params[i] = value;
    }
}