package processSimulation.processEquipment.pipeline;

import processSimulation.processEquipment.stream.*;
import fluidMechanics.flowSystem.onePhaseFlowSystem.pipeFlowSystem.*;
/**
 *
 * @author  esol
 * @version 
 */
public class OnePhasePipeLine extends Pipeline{

    /** Creates new OnePhasePipeLine */
    public OnePhasePipeLine() {
    }
    
    public OnePhasePipeLine(Stream inStream) {
        super(inStream);
        pipe = new PipeFlowSystem();
    }
    
    public void createSystem(){
        
    }
    
    public void run(){
        super.run();
        pipe.solveSteadyState(1);
        pipe.print();
        pipe.getDisplay().createNetCdfFile(fileName);
    }
}
