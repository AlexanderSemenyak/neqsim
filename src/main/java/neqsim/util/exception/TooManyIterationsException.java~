package util.exception;

/**
 *
 * @author  Even Solbraa
 * @version 
 */
public class IterationException extends util.exception.ThermoException {

    /**
     * Creates new <code>IterationException</code> without detail message.
     */
    public IterationException() {
    }


    /**
     * Constructs an <code>IterationException</code> with the specified detail message.
     * @param msg the detail message.
     */
    public IterationException(String msg) {
        super(msg);
    }
}


