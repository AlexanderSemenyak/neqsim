package util.exception;

/**
 *
 * @author  Even Solbraa
 * @version 
 */
public class VolumeIsNaNException extends util.exception.ThermoException {

    /**
     * Creates new <code>VolumeIsNaNException</code> without detail message.
     */
    public VolumeIsNaNException() {
    }


    /**
     * Constructs an <code>VolumeIsNaNException</code> with the specified detail message.
     * @param msg the detail message.
     */
    public VolumeIsNaNException(String msg) {
        super(msg);
    }
}


